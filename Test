   if v.Name == "Judgement Cut" then
       v.Parent = LP.Character
   end
end
for i = 1, 15 do
task.wait()
game:GetService("Players").LocalPlayer.Character:FindFirstChild("Judgement Cut").LocalScript.Event:FireServer()
end
end)

